{
  "name": "farmlokal",
  "version": "1.0.0",
  "description": "Full-stack application with Node.js, React, MySQL, and Redis",
  "main": "backend/src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/src/server.js",
    "start": "node backend/src/server.js",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm install && npm run build",
    "install-all": "npm install && cd frontend && npm install",
    "db:migrate": "node backend/src/database/migrations/run-migrations.js",
    "db:seed": "node backend/src/database/seeders/seed-products.js",
    "postinstall": "cd frontend && npm install",
    "test": "jest"
  },
  "keywords": ["oauth2", "redis", "mysql", "express", "react"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "opossum": "^8.1.3",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0"
  }
}
