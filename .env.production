# Production environment variables for Render
# These will be set in Render dashboard

# Backend - Set in Render Service Settings
NODE_ENV=production
PORT=5000

# Database - Automatically set from Render PostgreSQL/MySQL
# DB_HOST=
# DB_PORT=
# DB_USER=
# DB_PASSWORD=
# DB_NAME=

# Redis - Automatically set from Render Redis
# REDIS_HOST=
# REDIS_PORT=
# REDIS_PASSWORD=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Circuit Breaker
CIRCUIT_BREAKER_TIMEOUT=3000
CIRCUIT_BREAKER_ERROR_THRESHOLD=50
CIRCUIT_BREAKER_RESET_TIMEOUT=30000

# Cache Configuration
CACHE_TTL=300
TOKEN_CACHE_TTL=3600

# Logging
LOG_LEVEL=info

# Frontend - Set in Render Static Site Settings
# REACT_APP_API_URL=https://farmlokal-backend.onrender.com
